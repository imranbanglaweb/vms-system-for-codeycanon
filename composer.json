{
    "name": "inayafleet360/tms-transport-management-system",
    "type": "project",
    "description": "InayaFleet360 - All-in-One Fleet & Transport Automation System. A comprehensive Laravel-based transport and fleet management system.",
    "keywords": ["laravel", "transport-management", "fleet-management", "vehicle-management", "tms", "vms", "driver-management", "logistics", "inayafleet360"],
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "barryvdh/laravel-dompdf": "^2.2",
        "beyondcode/laravel-websockets": "^2.0@beta",
        "doctrine/dbal": "^3.6",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0",
        "laravel-notification-channels/webpush": "^7.1",
        "laravel/framework": "^9.0|^10.0",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.0",
        "laravelcollective/html": "^6.4",
        "maatwebsite/excel": "^3.1",
        "mews/purifier": "^3.4",
        "nesbot/carbon": "^2.72",
        "phpoffice/phpspreadsheet": "^1.29",
        "pusher/pusher-php-server": "^7.2",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-permission": "^5.10",
        "stichoza/google-translate-php": "^5.3",
        "thecodingmachine/safe": "^3.3",
        "yajra/laravel-datatables-oracle": "^10.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.10",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Notify.php",
            "app/Helpers/MenuHelper.php",
            "app/Helpers/TranslationHelpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
